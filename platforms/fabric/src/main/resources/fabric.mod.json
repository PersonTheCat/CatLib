{
  "schemaVersion": 1,
  "id": "${mod_id}",
  "version": "${mod_version}",
  "name": "${mod_name}",
  "description": "${mod_description}",
  "authors": [
    "${mod_authors}"
  ],
  "contact": {
    "homepage": "${mod_download}",
    "sources": "${mod_sources}"
  },
  "license": "GNUv3",
  "environment": "*",
  "entrypoints": {
    "main": [
      "personthecat.catlib.CatLib"
    ],
    "modmenu": [
      "personthecat.catlib.config.ModMenuIntegration"
    ]
  },
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric": "*",
    "minecraft": "${mc_version}",
    "cloth-config2": "${cloth_version}",
    "modmenu": "${mod_menu_version}",
    "java": ">=8"
  },
  "custom": {
    "modmenu": {
      "parent": {
        "id": "${mod_id}",
        "name": "${mod_name}",
        "description": "${mod_description}",
        "badges": [ "library" ]
      }
    }
  }
}